{
    "eslint.enable": true,
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
      "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

      "editor.codeActionsOnSave": {
        "source.fixAll": "always",
      },
      "editor.formatOnSave": true,
  }