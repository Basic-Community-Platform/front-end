{
	"root": true,
	"extends": [
		"next/core-web-vitals",
		"next",
		"prettier",
		"plugin:@typescript-eslint/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"env": {
		"node": true,
		"browser": true
	},
	"rules": {
		"no-console": "warn",
		"@typescript-eslint/no-explicit-any": "warn",
		"@typescript-eslint/no-unsafe-return": "warn"
	}
}